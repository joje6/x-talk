{
  "presets": [
    [
      "es2015", {
        "modules": false
      }
    ], [
      "env", {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "Safari >= 7",
            "Explorer >= 11",
            "iOS >= 9",
            "Android >= 4"
          ]
        },
        "debug": false
      }
    ]
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "transform-runtime", {
        "polyfill": true,
        "regenerator": true
      }
    ]
  ]
}
