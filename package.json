{
  "name": "x-talk",
  "version": "0.0.0",
  "private": true,
  "description": "x-talk app",
  "repository": "joje6/x-talk",
  "license": "MIT",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm run dist",
    "start": "static-server docs -p 9000 --not-found ./docs/index.html -i index.html -f",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --port 9000 --config build/webpack.dev.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "dist:storybook": "build-storybook -c .storybook -o storybook",
    "dist:web": "cross-env NODE_ENV=production webpack --config build/webpack.web.config.js",
    "babel": "rm -rf ./lib && babel ./src -d ./lib --source-maps inline --copy-files",
    "watch": "rm -rf ./lib && babel ./src -d ./lib --source-maps inline --copy-files -w",
    "dist": "npm run babel && cross-env NODE_ENV=production webpack --config build/webpack.dist.config.js",
    "lint": "eslint --ext .js,.vue src/",
    "lint:fix": "eslint --ext .js,.vue src/ --fix",
    "test": "rm -rf ./.test && karma start test/karma.conf.js --reporters html,dots"
  },
  "peerDependencies": {
    "vue": "2.x"
  },
  "dependencies": {
    "@vue/web-component-wrapper": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "document-register-element": "^1.11.0",
    "moment": "^2.22.2",
    "sweetalert": "^2.1.0",
    "x-router": "^0.5.23",
    "x-talk-connect": "github:joje6/x-talk-connect"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-knobs": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addon-notes": "^4.0.0-alpha.14",
    "@storybook/vue": "^4.0.0-alpha.14",
    "avoriaz": "^6.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.3",
    "eslint": "^4.19.1",
    "eslint-config-esnext": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "less-plugin-clean-css": "^1.5.1",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.2.3",
    "static-server": "^2.2.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.6",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "README.md"
  ]
}
